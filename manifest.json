{
  "manifest_version": 3,
  "name": "Close Tabs by Keywords",
  "version": "1.0",
  "description": "Automatically closes tabs with specified keywords in the URL.",
  "permissions": ["tabs", "storage"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  }
}

